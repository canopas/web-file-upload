{
  "name": "@canopassoftware/react-file-upload",
  "version": "1.2.0",
  "description": "Show the preview of file and manage files data to upload",
  "main": "index.umd.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canopas/react-file-upload.git"
  },
  "keywords": [
    "file-upload",
    "image-upload",
    "file-management",
    "file-preview",
    "image-preview"
  ],
  "author": "dharti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/canopas/react-file-upload/issues"
  },
  "homepage": "https://github.com/canopas/react-file-upload#readme",
  "scripts": {
    "dev": "next dev",
    "build": "npm run build-css && vite build && cp -r ./dist/* ./",
    "start": "next start",
